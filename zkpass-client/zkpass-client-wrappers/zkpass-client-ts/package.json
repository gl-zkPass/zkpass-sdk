  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/ffi-napi": "^4.0.10",
    "@types/jest": "^29.5.11",
    "@types/ref-napi": "^3.0.12",
    "dotenv-flow": "^4.1.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "ffi-napi": "^4.0.3",
    "jose": "^4.15.5",
    "patch-package": "^8.0.0",
    "ref-napi": "^3.0.3",
    "ref-struct-napi": "^1.1.1"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "tests/MockVariable",
