###############################################################################
#                                ↓ REQUIRED ↓                                 #
###############################################################################
# KMS: Leveraging AWS KMS to asynchronously generate keys
# NATIVE: zero-dependency, local, synchronous key generation
KEY_SERVICE=KMS
# KEY_SERVICE=NATIVE

# The file to write the generated keys to
JWT_OUTPUT=key-pairs.json
PUBLIC_KEY_OUTPUT=public-keys.json

###############################################################################
#                             ↓ OPTIONAL (KMS) ↓                              #
###############################################################################
AWS_KMS_ACCESS_KEY_ID=AKIASOMEACCESSKEYID
AWS_KMS_SECRET_ACCESS_KEY=SECRETACCESSKEYSECRETACCESSKEY
AWS_KMS_ENCRYPT_KEY_ID=KEYID_KEYID_KEYID
AWS_KMS_SIGN_KEY_ID=KEYID_KEYID_KEYID
AWS_KMS_REGION=us-east-2

###############################################################################
#                             ↓ OPTIONAL (S3)  ↓                              #
###############################################################################
AWS_S3_ACCESS_KEY_ID=AKIASOMEACCESSKEYID
AWS_S3_SECRET_ACCESS_KEY=SECRETACCESSKEYSECRETACCESSKEY
AWS_S3_REGION=us-east-2
AWS_S3_BUCKET=some-bucket
